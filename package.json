{
  "name": "replat",
  "version": "0.0.2",
  "description": "Boilerplate for Redux/React project",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/konstantin24121/replat.git",
    "type": "git"
  },
  "author": "VinstonWolf <konstantin24121@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "better-npm-run start-dev",
    "start:prod": "npm run build && better-npm-run start-prod",
    "build": "better-npm-run build",
    "lint": "npm run eslint && npm run stylelint",
    "stylelint": "stylelint src/**/*.css",
    "eslint": "eslint src"
  },
  "betterScripts": {
    "start-dev": {
      "command": "webpack-dashboard -- node ./implements/dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --inline --progress --display-error-details --config implements/webpack.common.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-prod": {
      "command": "node ./implements/prod-server.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 3000
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "express": "^4.14.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-diff-monitor": "^5.0.5",
    "redux-devtools-dispatch": "^2.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-multiple-monitors": "^1.0.1",
    "redux-slider-monitor": "^1.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "^0.0.13",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.7.0",
    "eslint-plugin-import": "^2.0.1",

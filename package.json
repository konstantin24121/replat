  "scripts": {
    "start": "better-npm-run start-dev",
    "build": "better-npm-run build",
    "lint": "npm run eslint && npm run stylelint",
    "stylelint": "stylelint src/*.css --config .stylelintrc --syntax postcss",
    "eslint": "eslint -c .eslintrc src"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --verbose --colors --inline --progress --display-error-details --config implements/webpack.common.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./implements/dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    }
  },
  "dependencies": {
